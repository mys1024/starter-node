{
  "name": "starter-node-package",
  "type": "module",
  "version": "0.0.0",
  "packageManager": "pnpm@7.28.0",
  "description": "A starter template for Node.js package.",
  "author": "mys1024",
  "license": "MIT",
  "homepage": "https://github.com/mys1024/starter-node-package#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/mys1024/starter-node-package.git"
  },
  "keywords": [
    "template"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsup src/index.ts --dts --format cjs,esm --clean",
    "test": "vitest",
    "test:cover": "vitest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.36.0",
    "@vitest/coverage-c8": "^0.29.2",
    "eslint": "^8.35.0",
    "tsup": "^6.6.3",
    "tsx": "^3.12.3",
    "typescript": "^4.9.5",
    "vitest": "^0.29.2"
  }
}
